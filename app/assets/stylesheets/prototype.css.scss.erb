#titlebar {
  background: #343434;
  height:38px;
  h1 {
    font-size: 25px;
    font-weight: bold;
    background: -webkit-gradient(linear, left top, left bottom, from(#FFF), to(#d4d1d2));
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    font-family: 'Source Sans Pro', sans-serif;
    padding: 0px;
    margin: 0px;
    padding-top: 3px;
    padding-left: 5px;
  }
}
.ui-dialog .ui-dialog-title { 
  color: #fff;
  text-shadow: 2px 2px 2px #000;
}
#toolbar {
  background-image: url(<%= asset_path 'sub_menu_bar_tile.png' %>);
  height: 44px;
  -webkit-box-shadow: 0px 1px #000000;
   -moz-box-shadow: 0px 1px #000000;
        box-shadow: 0px 1px #000000;
  a {
      display:block;
      margin-top: 7px;
      margin-right: 5px;
      text-decoration: none;
      float: right;
      background-color: #7fbf4d;
      background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #7fbf4d), color-stop(100%, #63a62f));
      background-image: -webkit-linear-gradient(top, #7fbf4d, #63a62f);
      background-image: -moz-linear-gradient(top, #7fbf4d, #63a62f);
      background-image: -ms-linear-gradient(top, #7fbf4d, #63a62f);
      background-image: -o-linear-gradient(top, #7fbf4d, #63a62f);
      background-image: linear-gradient(top, #7fbf4d, #63a62f);
      border: 1px solid #63a62f;
      border-bottom: 1px solid #5b992b;
      border-radius: 3px;
      -webkit-box-shadow: inset 0 1px 0 0 #96ca6d;
      box-shadow: inset 0 1px 0 0 #96ca6d;
      color: #fff;
      font: bold 11px/1 "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif;
      padding: 7px 0 8px 0;
      text-align: center;
      text-shadow: 0 -1px 0 #4c9021;
      width: 100px;
  }
}


#shadow {
  background: url(<%= asset_path 'drop_shadow.png' %>) top left repeat-x;
  margin-top:-1px;
  height: 8px;
  width: 100%;
  position: absolute;
}
#shadow_h {
  background: url(<%= asset_path 'drop_shadow_h.png' %>) top left repeat-y;
  width: 10px;
  position: absolute;
  margin-top:2px;
}
.ui-dialog {
  -webkit-box-shadow: 5px 5px 5px #888;
}
#editor {
  margin-left: auto;
  margin-right: auto;
  overflow: auto;
  width: 100%;
  #components {
    float: left;
    width: 312px;
    background: #f3f3f3;
    border-right: solid 1px #a5a5a5;
    .ui-widget-content {
      background: transparent;
      border: none;
    }
    #tabs {
      height: 50px;
      ul.tabs {
        padding: 0px;
        margin: 0px;
        background: #e4e4e4;
        padding-top: 10px;
        list-style: none;
        display: block;
        text-align: center;
        border-bottom: solid 1px #bbbbbb;
        li {
          background: none;
          border: none;
          margin: 0px;
          border-top-left-radius: 15px;
          border-top-right-radius: 15px;
          padding: 0px;
          height: 40px;
          width: 130px;
          display: inline-block;
          a {
            color: #999999;
            margin-top: 13px;
            display: block;
            font-size: 14px;
            font-weight: bold;
            text-decoration: none;
          }
          &.ui-tabs-selected {
            background: #F3F3F3;
            border: solid 1px #bbbbbb;
            border-bottom: none;
            a {
              color: #000;
              background: #f3f3f3;
              height: 30px;
            }
          }
        }
      }
    }
  }
#library {
  ul {
    list-style: none;
    margin: 0px;
    padding: 0px;
    margin:20px;
    li {
      display: block ;
      padding: 15px;
      .image {
        width: 100px;
        height: 60px;
      }
      .label {
        color: #6B6A6A;
        font-size: 12px;
        text-align: center;
      }
        &.SOAP_endpoint .image {
          background: url(<%= asset_path 'nodes/soap_endpoint.png' %>) top left no-repeat;
         }
    }
  }
}

#routes {
    h2 {
      padding-left: 5px;
    }
    ul {
      list-style: none;
      padding: 0px;
      margin: 0px;
      li {
        padding: 12px;
        font-weight: bold;
        border-bottom: solid 1px #bbbbbb;
        display: block;
        &.first { 
          border-top: solid 1px #bbbbbb;
        }
        &.active,&:hover {
          background: #e4e4e4;
        }
      }
    }
}
